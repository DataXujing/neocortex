{
  "name": "neuralnet-predict-js",
  "version": "0.0.0",
  "description": "Run trained neural networks in the browser or node.js",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build-webpack": "webpack src/index.js build/neuralnet-predict.min.js --config webpack.config.js",
    "build-babel": "rm -rf dist && node_modules/.bin/babel src --out-dir dist",
    "build": "npm run build-babel && npm run build-webpack",
    "prepublish": "npm run build",
    "test": "mocha --reporter spec --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transcranial/neuralnet-predict-js.git"
  },
  "keywords": [
    "neuralnet",
    "neuralnets",
    "neuralnetworks",
    "neural",
    "network",
    "networks",
    "deep",
    "machine",
    "learning"
  ],
  "author": "Leon Chen <lchen3@gmail.com> (@transcranial)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/transcranial/neuralnet-predict-js/issues"
  },
  "homepage": "https://github.com/transcranial/neuralnet-predict-js#readme",
  "devDependencies": {
    "almost-equal": "^1.0.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "eslint": "^1.5.0",
    "mocha": "^2.3.2",
    "simd": "^2.0.0",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "cwise": "^1.0.8",
    "ndarray": "^1.0.18",
    "ndarray-matrix-vector-product": "^1.0.0",
    "ndarray-ops": "^1.2.2",
    "ndarray-pack": "^1.1.1",
    "ndarray-unpack": "^1.0.0",
    "raw-loader": "^0.5.1"
  }
}
