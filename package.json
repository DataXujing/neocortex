{
  "name": "neuralnet-predict-js",
  "version": "0.0.0",
  "description": "Run trained neural networks in the browser or node.js",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build-browser": "browserify src/neuralnet.js -t babelify -t brfs -p [minifyify --no-map] --standalone NeuralNet -o build/neuralnet-predict.min.js",
    "build-node": "rm -rf dist && node_modules/.bin/babel src --out-dir dist",
    "build": "npm run build-node && npm run build-browser",
    "prepublish": "npm run build",
    "test": "mocha --reporter spec --compilers js:babel/register",
    "examples-server": "npm run build-browser && cp build/neuralnet-predict.min.js examples && node examples/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transcranial/neuralnet-predict-js.git"
  },
  "keywords": [
    "neuralnet",
    "neuralnets",
    "neuralnetworks",
    "neural",
    "network",
    "networks",
    "deep",
    "machine",
    "learning"
  ],
  "author": "Leon Chen <lchen3@gmail.com> (@transcranial)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/transcranial/neuralnet-predict-js/issues"
  },
  "homepage": "https://github.com/transcranial/neuralnet-predict-js#readme",
  "devDependencies": {
    "almost-equal": "^1.0.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babelify": "^6.3.0",
    "brfs": "^1.4.1",
    "browserify": "^11.2.0",
    "concat-stream": "^1.5.0",
    "eslint": "^1.5.0",
    "express": "^4.13.3",
    "minifyify": "^7.1.0",
    "mocha": "^2.3.2",
    "raw-loader": "^0.5.1",
    "simd": "^2.0.0"
  },
  "dependencies": {
    "cwise": "^1.0.8",
    "ndarray": "^1.0.18",
    "ndarray-blas-level1": "^1.1.1",
    "ndarray-convolve": "^2.0.0",
    "ndarray-ops": "^1.2.2",
    "ndarray-pack": "^1.1.1",
    "ndarray-unpack": "^1.0.0",
    "superagent": "^1.4.0"
  }
}
